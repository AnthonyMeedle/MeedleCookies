<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">
	
	  <route id="meedlecookies.configuration.default" path="/admin/module/MeedleCookies" methods="get">
	    <default key="_controller">MeedleCookies:MeedleCookiesConfig:default</default>
	  </route>
	  
	  <route id="meedlecookies.configuration.save" path="/admin/module/MeedleCookies" methods="post">
	    <default key="_controller">MeedleCookies:MeedleCookiesConfig:save</default>
	  </route>
    <!--

    if a /admin/module/meedlecookies/ route is provided, a "Configuration" button will be displayed
    for the module in the module list. Clicking this button will invoke this route.

    <route id="my_route_id" path="/admin/module/meedlecookies">
        <default key="_controller">MeedleCookies\Full\Class\Name\Of\YourConfigurationController::methodName</default>
    </route>

    <route id="my_route_id" path="/admin/module/meedlecookies/route-name">
        <default key="_controller">MeedleCookies\Full\Class\Name\Of\YourAdminController::methodName</default>
    </route>

    <route id="my_route_id" path="/my/route/name">
        <default key="_controller">MeedleCookies\Full\Class\Name\Of\YourOtherController::methodName</default>
    </route>

    ...add as many routes as required.

    <route>
    ...
    </route>
    -->

</routes>
